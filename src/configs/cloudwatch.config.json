{
  "agent": {
      "metrics_collection_interval": 10,
      "logfile": "/var/logs/amazon-cloudwatch-agent.log"
  },
  "logs": {
      "logs_collected": {
          "files": {
              "collect_list": [
                  {
                      "file_path": "/home/ubuntu/webapp/logs/webapp.log",
                      "log_group_name": "csye6225",
                      "log_stream_name": "webapp"
